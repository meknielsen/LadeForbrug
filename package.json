    "@smui/form-field": "^7.0.0-beta.16",
    "@smui/icon-button": "^7.0.0-beta.15",
    "@smui/layout-grid": "^7.0.0-beta.15",
    "@smui/menu": "^7.0.0-beta.16",
    "@smui/paper": "^7.0.0-beta.15",
    "@smui/select": "^7.0.0-beta.16",
